{
  "name": "chatgpt-qa-agent",
  "version": "1.0.0",
  "description": "A standalone Q&A recommendation web app that generates personalized questions and recommendations",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js",
    "python:dev": "cd python-service && python3 -m uvicorn main:app --host 0.0.0.0 --port 8000"
  },
  "keywords": [
    "recommendation",
    "qa-agent",
    "web-app",
    "questionnaire"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "e2b": "^2.7.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
